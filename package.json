{
  "name": "ChangePackageName",
  "version": "0.5.0",
  "description": "Start react development here...",
  "main": "src/libs/samples.jsx",
  "bin": {
    "new-react-component": "bin/new-react-component.js"
  },
  "scripts": {
    "preinstall": "npm -v & bower -v",
    "preversion": "npm run build",
    "postversion": "git push --tags && npm run git-push",
    "dev": "browser-sync start --config bs-config.js & webpack -d",
    "git-commit": "git add -A . && git commit -a -m 'Release packaged'",
    "git-push": "git push origin develop",
    "build": "rm -r app/ && webpack -p --progress --colors",
    "build-html": "rm -r component/ && webpack -p --progress --colors --config html.webpack.config.js",
    "release": "npm run build && npm run git-commit && npm run version:patch",
    "version:patch": "npm version patch && git push --tags",
    "liveview": "browser-sync start --config bs-config.js",
    "updates": "npm update && bower update",
    "setup-react-start": "npm link"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/foluke-ui-kit/start-react.git"
  },
  "keywords": [
    "react"
  ],
  "author": "shawn sandy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/foluke-ui-kit/start-react/issues"
  },
  "homepage": "https://github.com/foluke-ui-kit/start-react",
  "devDependencies": {
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.2.2",
    "browser-sync": "^2.9.6",
    "classnames": "^2.1.3",
    "css-loader": "^0.14.4",
    "data-attrs-to-js": "^1.1.1",
    "eslint": "^1.6.0",
    "eslint-loader": "^1.1.0",
    "eslint-plugin-react": "^3.5.1",
    "file-loader": "^0.8.4",
    "fs-extra": "^0.24.0",
    "gulp": "^3.8.9",
    "inquirer": "^0.10.1",
    "jquery": "^2.1.4",
    "jsx-loader": "^0.13.2",
    "node-libs-browser": "^0.5.2",
    "object-assign": "^2.0.0",
    "react-router": "^1.0.0-rc3",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "watchify": "^3.2.1",
    "webpack": "^1.9.10"
  },
  "dependencies": {
    "react": "^0.14.0",
    "react-dom": "^0.14.0"
  }
}
